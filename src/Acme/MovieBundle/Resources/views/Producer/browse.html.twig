{% extends "SurvosAdminBundle::search_layout.html.twig" %}

{% block results %}

<table class="condensed-table bordered-table">

<thead>
  <tr>
      <th>Id</th>
      <th>Name</th>
      <th>IsPublished</th>
    </tr>
</thead>

<tbody>
{% for r in pager.getCurrentPageResults() %}
<tr>
    <td><a href="{{ path('acme_movie_Producer_edit', {'id': r.Id}) }}">{{ r.Id }}</a></td>
    <td>{{ r.Name }}</td>
    <td>{{ r.IsPublished }}</td>
</tr>
{% endfor %}
</table>

{% endblock %}

